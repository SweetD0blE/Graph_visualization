let vQvdPath = 'lib://FIR_DATA/TreeConnection/LogsVisit.qvd';
let vQvdExists = FileSize('$(vQvdPath)');

if len('$(vQvdExists)') > 0 then
                MashupVisits:
    Load
               UserId,
        VisitTime
    from [$(vQvdPath)](qvd);
else 
                // Если размер 0 то создаем
                MashupVisits:
    load
               '' as UserID,
        Now() as VisitTime
    AutoGenerate 0;
endif;

Concatenate (MashupVisits)
Add Load
                SubField('$(vUser)', 'UserId=', 2) as UserId, // Тут еще поделить нужно чтобы логин пользователя получить.
    now() as VisitTime
AutoGenerate 1;

store MashupVisits into [lib://FIR_DATA/TreeConnection/LogsVisit.qvd](qvd);
